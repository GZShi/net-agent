{
  "tunnel": {
    "address": "localhost:2035",
    "password": "",

    // 向服务器注册的虚拟子域名
    // 如果已经被注册，服务器会自动增加数字后缀，例如：test_1
    // 注册成功后，其它节点可以通过 test.tunnel 这样的虚拟域名访问本机的services
    "vhost": "test"
  },

  "services": [{
    "enable": true,
    "description": "将本地网络用socks5协议暴露在test.tunnel:1081上",
    "type": "socks5",
    "param": {
      "listen": "tunnel:1081",
      "username": "",
      "password": ""
    }
  }, {
    "enable": true,
    "description": "将test.tunnel:1081的服务用本地地址localhost:1082进行代理",
    "type": "portproxy",
    "param": {
      "listen": "localhost:2037",
      "target": "test.tunnel:1081"
    }
  }]
}