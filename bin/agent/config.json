{
  "tunnel": {
    "network": "tcp4",
    "addressx": "localhost:2035",
    "address": "ws://localhost:2035/tunnel",
    "password": "",

    // 向服务器注册的虚拟子域名
    // 如果已经被注册，服务器会自动增加数字后缀，例如：test_1
    // 注册成功后，其它节点可以通过 test.tunnel 这样的虚拟域名访问本机的services
    "vhost": "test"
  },

  "services": [{
    "enable": true,
    "description": "反射socks5服务",
    "type": "socks5",
    "param": {
      "listen": "tunnel:1081",
      "username": "ping",
      "password": "pong"
    }
  }, {
    "enable": false,
    "description": "代理服务",
    "type": "portproxy",
    "param": {
      "listen": "localhost:2037",
      "target": "test.tunnel:1081"
    }
  }, {
    "enable": true,
    "description": "即时通信服务",
    "type": "chatserver",
    "param": {}
  }]
}